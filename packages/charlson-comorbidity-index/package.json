{
  "name": "@bonfhir/charlson-comorbidity-index",
  "description": "Charlson Comorbidity Index calculator.",
  "version": "0.1.0-alpha.4",
  "repository": "https://github.com/bonfhir/bonfhir.git",
  "license": "APACHE-2.0",
  "type": "module",
  "scripts": {
    "build": "yarn clean && tsup r4b/index.ts --format esm,cjs --out-dir dist/r4b --shims --dts --tsconfig tsconfig.build.json",
    "check": "prettier --check ./**/*.ts && eslint ./**/*.ts && tsc --noEmit",
    "clean": "rimraf dist/",
    "format": "prettier --loglevel warn --write ./**/*.ts && eslint --fix ./**/*.ts",
    "package:create": "yarn build && node package.js pack",
    "package:publish": "yarn build && node package.js publish",
    "test": "jest"
  },
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "@bonfhir/eslint-config": "^1.0.0",
    "@bonfhir/medplum": "^1.0.0-alpha.9",
    "@bonfhir/prettier-config": "^1.0.0",
    "@bonfhir/test-support": "^1.0.0-alpha.9",
    "@bonfhir/typescript-config": "^1.0.0",
    "@medplum/core": "^2.0.1",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/fhir": "^0.0.35",
    "@types/lodash": "^4",
    "@types/node": "^18.11.18",
    "@types/rimraf": "^3",
    "eslint": "^8.30.0",
    "fetch-vcr": "^3.2.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.6.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@bonfhir/core": "^1.0.0-alpha.8",
    "lodash": "^4.17.21"
  },
  "prettier": "@bonfhir/prettier-config"
}
