{
  "name": "@bonfhir/core",
  "description": "Core package for Bonfhir.",
  "version": "1.0.0-alpha.8",
  "repository": "https://github.com/bonfhir/bonfhir.git",
  "license": "APACHE-2.0",
  "type": "module",
  "scripts": {
    "build": "yarn clean && npm run rollup",
    "check": "prettier --check ./**/*.ts && eslint ./**/*.ts && tsc --noEmit",
    "clean": "rimraf dist/",
    "rollup": "rollup --config rollup.config.mjs",
    "codegen": "yarn workspace @bonfhir/codegen run dev run -d \"${PWD}/../../fhir/r4b/definitions/**/*.json\" -t \"${PWD}/r4b/**/*.hbs\" --helpers \"${PWD}/template-helpers.js\" -p 'prettier --write %files%'",
    "package:create": "node package.js pack",
    "package:publish": "node package.js publish",
    "test": "jest"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    },
    "./r4b": {
      "types": "./dist/esm/r4b/index.d.ts",
      "require": "./dist/cjs/r4b/index.cjs",
      "import": "./dist/esm/r4b/index.mjs"
    }
  },
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "@bonfhir/codegen": "^1.0.0-alpha.2",
    "@bonfhir/eslint-config": "^1.0.0",
    "@bonfhir/prettier-config": "^1.0.0",
    "@bonfhir/typescript-config": "^1.0.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/fhir": "^0.0.35",
    "@types/node": "^18.11.18",
    "@types/rimraf": "^3",
    "date-fns": "^1.30.1",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.14.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "fhirpath": "^3.3.1",
    "html-entities": "^2.3.3"
  },
  "prettier": "@bonfhir/prettier-config",
  "sideEffects": true
}
