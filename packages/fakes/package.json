{
  "name": "@bonfhir/fakes",
  "version": "0.0.0",
  "description": "Fakers for Bonfhir.",
  "type": "module",
  "main": "dist/index.js",
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "build": "yarn clean && tsc",
    "check-all": "prettier --check . '!**/*.hbs' && eslint . && tsc --noEmit",
    "clean": "rimraf dist/",
    "codegen": "yarn workspace @bonfhir/codegen run dev run -d '/workspace/fhir/**/*.json' --t '/workspace/packages/fakes/**/*.hbs' -p 'prettier --write %files%'"
  },
  "dependencies": {
    "@bonfhir/core": "*",
    "@faker-js/faker": "^7.6.0"
  },
  "devDependencies": {
    "@bonfhir/codegen": "*",
    "@bonfhir/eslint-config": "*",
    "@bonfhir/prettier-config": "*",
    "@bonfhir/typescript-config": "*",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "@types/fhir": "^0.0.35",
    "@types/node": "^18.11.18",
    "@types/rimraf": "^3",
    "eslint": "^8.30.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "prettier": "@bonfhir/prettier-config"
}
