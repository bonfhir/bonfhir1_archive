export * from "./{{camelcase name}}";
{{#writeFiles "{{camelcase name}}.ts"}}
/**
 * {{ description }}
 * {{ url }}
 */
export const {{safeNameAsVar name}}Code = {
  {{#each (recursiveFlatten expansion.contains "contains")}}
    /**
     * {{display}}
     */
    {{#if (safeNameAsVar display)}}{{safeNameAsVar display}}{{else}}"{{code}}"{{/if}}: "{{code}}",
    
  {{/each}}
} as const;

